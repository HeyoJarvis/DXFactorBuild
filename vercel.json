{
  "version": 2,
  "name": "heyjarvis",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/chat",
      "dest": "/chat.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
